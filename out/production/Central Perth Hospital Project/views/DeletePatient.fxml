<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="DeletePatient"
          fx:id="deletePatientBack"
          alignment="CENTER"
          prefHeight="200.0" prefWidth="200.0" vgap="30" hgap="30" style="-fx-background-color:#800080">

    <!--<Label GridPane.halignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="0" prefHeight="20" prefWidth="80">Patient ID : </Label>-->
    <TextField fx:id="patientId" GridPane.halignment="CENTER" maxWidth="80" minWidth="80" GridPane.columnSpan="2" GridPane.columnIndex="0" promptText="PATIENT ID" GridPane.rowIndex="0" prefHeight="20" prefWidth="80"></TextField>

    <Button fx:id="confirm" onAction="#confirmClick" prefWidth="80" prefHeight="30" GridPane.rowIndex="1" GridPane.columnIndex="0">CONFIRM</Button>
    <Button fx:id="cancel" onAction="#cancelClick" prefWidth="80" prefHeight="30" GridPane.rowIndex="1" GridPane.columnIndex="1">CANCEL</Button>

</GridPane>
