<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>

<?import javafx.geometry.Insets?>

<GridPane alignment="CENTER" fx:id="mainMenuPage" fx:controller="Controller" maxHeight="-Infinity" vgap="40" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx"
          style="-fx-background-color: #800080" xmlns:fx="http://javafx.com/fxml">

    <padding><Insets bottom="10" top="10" left="10" right="10"></Insets></padding>

    <Button text="MAIN MENU" alignment="CENTER" GridPane.columnSpan="1" GridPane.rowIndex="0" GridPane.columnIndex="0"  prefHeight="40" prefWidth="150"/>
    <Button fx:id="patientRegistration" text="PATIENT REGISTRATION"  onAction="#patientRegistrationClick" alignment="CENTER" GridPane.columnSpan="1" GridPane.rowIndex="1" GridPane.columnIndex="0"  prefHeight="40" prefWidth="150"/>
    <Button fx:id="configuration" onAction="#configurationClick" text="CONFIGURATION" alignment="CENTER" GridPane.columnSpan="1" GridPane.rowIndex="2" GridPane.columnIndex="0"  prefHeight="40" prefWidth="150"/>
    <Button fx:id="close" text="CLOSE" onAction="#closeClick" alignment="CENTER" GridPane.columnSpan="1" GridPane.rowIndex="3" GridPane.columnIndex="0"  prefHeight="40" prefWidth="150"/>

</GridPane>
